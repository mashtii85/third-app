{
  "presets": ["@babel/preset-env", "@babel/preset-react", "next/babel"],
  "plugins": [
    "@babel/plugin-transform-runtime",
    [
      "styled-components",
      {
        "ssr": true,
        "displayName": true,
        "preprocess": false
      }
    ]
  ],
  "overrides": [
    {
      "include": ["./node_modules"],
      "plugins": [
        [
          "babel-plugin-transform-require-ignore",
          {
            "extensions": [".css"]
          }
        ]
      ]
    }
  ]
}
