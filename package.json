{
  "name": "lms.realworldacademies.com",
  "license": "ISC",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "start": "next",
    "test": "npx eslint . --ext .js --ignore-path .gitignore",
    "test:fix": "npx eslint . --ext .js --ignore-path .gitignore --fix",
    "cy:start": "cypress open -C ./cypress-dev.json",
    "cy:start:serve": "cypress open -C ./cypress-local.json",
    "cy:test": "npm run package && start-server-and-test serve 5000 cy:run",
    "cy:run": "cypress run -C ./cypress-local.json",
    "clean-cache": "rimraf -rf ./node_modules/.cache",
    "build": "rimraf -rf ./.next && next build",
    "export": "next export -s"
  },
  "engines": {
    "node": "^14"
  },
  "lint-staged": {
    "**/*.js": [
      "npm test"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@drykiss/industry-ui": "^1.14.13",
    "@types/styled-components": "^5.1.12",
    "deepmerge": "^4.2.2",
    "graphql": "^15.5.1",
    "jsonwebtoken": "^8.5.1",
    "subscriptions-transport-ws": "^0.9.19"
  },
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@babel/eslint-parser": "^7.12.17",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-react": "^7.12.13",
    "@types/react": "17.0.19",
    "babel-plugin-styled-components": "^1.12.0",
    "babel-plugin-transform-require-ignore": "^0.1.1",
    "cypress": "^8.3.0",
    "cypress-dotenv": "^1.2.2",
    "dotenv": "^8.2.0",
    "eslint": "7.25.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-standard": "16.0.2",
    "eslint-config-standard-react": "11.0.1",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-promise": "5.1.0",
    "eslint-plugin-react": "7.23.2",
    "eslint-plugin-react-hooks": "4.2.0",
    "eslint-plugin-standard": "5.0.0",
    "fs-extra": "^9.1.0",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.3",
    "next-transpile-modules": "7.0.0",
    "prettier": "2.2.1",
    "prettier-config-standard": "^4.0.0",
    "push-dir": "^0.4.1",
    "serve": "^12.0.0",
    "typescript": "^4.3.5"
  }
}
